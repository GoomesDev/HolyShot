{
  "expo": {
    "name": "HolyShot",
    "slug": "app",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/HolyShot.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "androidStatusBar": {
        "hidden": true,
        "backgroundColor": "#000000"
      },
      "package": "holyshot.app"
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9690a94f-903a-4a94-9bd2-cd69500305b9"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
